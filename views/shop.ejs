<div class="banner">
  <img
    class="w-100 lazyautosizes lazyloaded d-none d-md-block"
    src="images/taryl_banner1.jpg"
    sizes="1191px"
  />
  <div class="centered">Luxury that Represents You.</div>
</div>

<div class="items">
  <% items.forEach(item =>{ %>
  <a href="shop/<%= item.id %>">
    <div id="wrapper">
      <img class="album-pic" src="<%= item.images[0] %>" alt="clothing" />
      <img class="album-pic-exit" src="<%= item.images[1] %>" />
    </div>

    <span><strong><%= item.item %></strong></span>
    <span>$<%= item.price %> USD</span>
  </a>

  <% }) %>
</div>

<div
  id="carouselExampleCaptions"
  class="carousel slide"
  data-bs-ride="carousel"
>
  <div class="carousel-indicators">
    <button
      type="button"
      data-bs-target="#carouselExampleCaptions"
      data-bs-slide-to="0"
      class="active"
      aria-current="true"
      aria-label="Slide 1"
    ></button>
    <button
      type="button"
      data-bs-target="#carouselExampleCaptions"
      data-bs-slide-to="1"
      aria-label="Slide 2"
    ></button>
    <button
      type="button"
      data-bs-target="#carouselExampleCaptions"
      data-bs-slide-to="2"
      aria-label="Slide 3"
    ></button>
  </div>
  <div class="carousel-inner">
    <div class="carousel-item active">
      <img
        src="  https://cdn.shopify.com/s/files/1/0587/5816/8785/files/DESKTOP_SLIDE_321ee6fa-4fe0-46e7-99bf-eb830e4eaf31_2048x.jpg?v=1641736376
        "
        class="d-block w-100"
        alt="..."
      />
      <div class="carousel-caption d-none d-md-block">
        <h5>What do you represent?</h5>
        <p>Luxury streetware to represent you.</p>
      </div>
    </div>
    <div class="carousel-item">
      <img
        src="https://cdn.shopify.com/s/files/1/0587/5816/8785/files/newD_2048x.jpg?v=1649263076
        "
        class="d-block w-100"
        alt="..."
      />
      <div class="carousel-caption d-none d-md-block">
        <h5>What do you represent?</h5>
        <p>Luxury streetware to represent you.</p>
      </div>
    </div>
    <div class="carousel-item">
      <img
        src="https://cdn.shopify.com/s/files/1/0181/2235/files/247_header-desktop.jpg?v=1635952392
        "
        class="d-block w-100"
        alt="..."
      />
      <div class="carousel-caption d-none d-md-block">
        <h5>What do you represent?</h5>
        <p>Luxury streetware to represent you.</p>
      </div>
    </div>
  </div>
  <button
    class="carousel-control-prev"
    type="button"
    data-bs-target="#carouselExampleCaptions"
    data-bs-slide="prev"
